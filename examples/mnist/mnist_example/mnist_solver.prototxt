net: "examples/mnist/mnist_example/mnist_train_test.prototxt"

test_iter: 100
test_interval: 20000
test_initialization: false

base_lr: 1e-3
momentum: 0.9
weight_decay: 5e-4

lr_policy: "multistep"
gamma: 0.8
stepvalue: 5000
stepvalue: 8000

display: 100

# The maximum number of iterations
max_iter: 10000
# snapshot intermediate results
snapshot: 10000
snapshot_prefix: "examples/mnist/mnist_example/mnist_train"

# solver mode: CPU or GPU
solver_mode: GPU
